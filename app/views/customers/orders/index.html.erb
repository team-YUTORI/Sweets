<div class="container">
<div class="row">
<div class="col-md-12">

  <table class="table table-hover">
    <thead>
      <th>
        注文日
        </th>
      <th>
        配送先
        </th>
      <th>
        注文商品
        </th>
      <th>
        支払い金額
        </th>
      <th>
        ステータス
        </th>
      <th>
        注文詳細
        </th>
      </thead>

  <%= @orders.each do |order| %>
    <tbody>
      <tr>
      <td>
        <%= order.created_at %>
       </td>
      <td>
        <%= order.delivery_addresses %>
        </td>
      <td>
        <%= order.item_id %>
        </td>
      <td>
        <%= order.payment %>
        </td>
      <td>
        <%= order.order_status%>
        </td>
        <%= link_to"Show" , order_path(order.id) %>
      <td>

        </td>
      </tr>
    </tbody>
   <% end %>
  </table>

</div>
</div>
</div>
